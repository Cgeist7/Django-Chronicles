--

CREATE TABLE personas_nation (
    id integer NOT NULL,
    name character varying(128) NOT NULL,
    description text NOT NULL,
    might smallint NOT NULL,
    intrigue smallint NOT NULL,
    magic smallint NOT NULL,
    wealth smallint NOT NULL,
    influence smallint NOT NULL,
    defense smallint NOT NULL,
    slug character varying(50) NOT NULL,
    story_id integer NOT NULL,
    image character varying(100) NOT NULL,
    CONSTRAINT personas_nation_defense_check CHECK ((defense >= 0)),
    CONSTRAINT personas_nation_influence_check CHECK ((influence >= 0)),
    CONSTRAINT personas_nation_intrigue_check CHECK ((intrigue >= 0)),
    CONSTRAINT personas_nation_magic_check CHECK ((magic >= 0)),
    CONSTRAINT personas_nation_might_check CHECK ((might >= 0)),
    CONSTRAINT personas_nation_wealth_check CHECK ((wealth >= 0))
);


